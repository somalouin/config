{
  "assistant": {
    "dock": "right",
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    },
    "version": "2"
  },
  "base_keymap": "VSCode",
  "ui_font_size": 14,
  "buffer_font_size": 12,
  "ui_font_family": "Zed Plex Sans",
  "formatter": "prettier",
  "gutter": {
    "line_numbers": true
  },
  "theme": {
    "mode": "system",
    "light": "Sandcastle",
    "dark": "Sandcastle"
  },
  "project_panel": {
    "dock": "left",
    "indent_size": 12
  }
}
